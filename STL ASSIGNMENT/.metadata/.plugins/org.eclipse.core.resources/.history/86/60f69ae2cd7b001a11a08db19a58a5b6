//Create STL list of string and add following names of your 10 friends.
//Sort the list and display in reverse order using reverse iterator.

#include <iostream>
#include<string>
#include<list>
using namespace std;

int main() {

	list<string> name;

	string frn_name;
	for(int i=0;i<3;i++)
	{
		cin>>frn_name;
		name.pop_back(name);
	}



	return 0;
}
