//Create STL list of string and add following names of your 10 friends.
//Sort the list and display in reverse order using reverse iterator.

#include <iostream>
#include<string>
#include<algorithm>
#include<list>
using namespace std;

int main() {

	list<string> name;

	string frn_name;
	for(int i=0;i<3;i++)
	{
		cin>>frn_name;
		name.push_back(frn_name);
	}
	list<string>::iterator itr1 = name.begin();
	list<string>::iterator itr2 = name.end();


	name.sort();

	list<string>::iterator itr = itr1;
	while(itr!=itr2)
	{
		cout<<*itr<<endl;
		itr1++;
	}



	return 0;
}
