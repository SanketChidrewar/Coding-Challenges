//4. Create STL list of int values. Add 100 random values in it (use rand() function of C). Display all
//unique elements in list. Hint: use set.

#include<iostream>
#include<set>
#include<list>

using namespace std;


using namespace std;

int main()
{
	list<int>mylist(100,0);
	for(int i : mylist)
	{
		mylist.insert(rand());
	}

	set<int>myset;
	int j=0;

	for(int i=0;i<100;i++)
	{
		myset[i] = mylist[i];
	}

	for(int i :myset)
	{
		j++;
		cout<<j <<" : "<<i<<endl;
	}

	//cout<<rand()<<endl;
	//cout<<rand();


	return 0;
}



