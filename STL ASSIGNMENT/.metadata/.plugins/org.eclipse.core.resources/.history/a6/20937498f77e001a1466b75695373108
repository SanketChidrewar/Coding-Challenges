//7. A text file contains set of integer (in range of 1 to 9) values separated by spaces. Read them into
//STL vector. Find the missing numbers from them.
//Input: 3 7 9 4 3 7 9 8 6 4 7 2
//Output: 1 5

#include<iostream>
#include <fstream>
#include<map>
#include <sstream>

using namespace std;

int main()
{
	ifstream is("src/numbers.txt");
	string numline;

	map<int,int> n;

	if(is.is_open())
	{
		while(getline(is,numline))
		{
			//cout<<numline<<endl;
			stringstream ss;
			ss<<numline;

			string temp;
			int found;
	        ss >> temp;
			while(!ss.eof())
			{
				if(stringstream(temp)>>found)
				{
					n[found]++;
				}
			}
		}
		is.close();
	}
	else
	{
		cout<<"FILE OPENING IS FAIL. "<<endl;
	}

	map<int,int>::iterator it = n.begin();

	return 0;
}


