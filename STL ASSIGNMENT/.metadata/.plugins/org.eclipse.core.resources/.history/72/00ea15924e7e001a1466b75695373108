//4. Create STL list of int values. Add 100 random values in it (use rand() function of C). Display all
//unique elements in list. Hint: use set.

#include<iostream>
#include<set>
#include<list>

using namespace std;

int main()
{
	list<int>mylist(100,0);

	list<int>::iterator it = mylist.begin();

	while(it!=mylist.end())
	{
		mylist.insert(it,rand());
		it++;
	}

	set<int>myset;

	it = mylist.begin();

	while(it!=mylist.end())
	{
		myset.insert(*it);
		it++;
	}

	int j=0;
	for(int i :myset)
	{
		j++;
		cout<<j <<" : "<<i<<endl;
	}

/*	int j=0;
	set<int>::iterator it1;
	it1 = myset.begin();
	while(it1!=myset.end())
	{
		j++;
		cout<<j<<" : "<<*it1<<endl;
		it1++;
	}*/


	//cout<<rand()<<endl;
	//cout<<rand();

	return 0;
}


