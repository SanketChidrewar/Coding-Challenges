#include "tapes.h"
#include"product.h"

tapes::tapes():product() {
	this->duration=0;
}

tapes::tapes(int duration,int price):product(price)
{
	this->duration=duration;
}

int tapes::getDuration() const {
	return duration;
}

void tapes::setDuration(int duration) {
	this->duration = duration;
}

