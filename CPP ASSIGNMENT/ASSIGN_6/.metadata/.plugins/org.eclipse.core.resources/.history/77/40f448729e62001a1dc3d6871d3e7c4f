#ifndef RECTANGLE_H_
#define RECTANGLE_H_
#include"shape.h"


class rectangle :public shape{
private:
	float side;
public:
	rectangle();

	circle(float side);
	float get_side();
	void set_side(float side);
	void accept();
	void display();
	float calc_area();
	float calc_peri();

	virtual ~rectangle();
};

#endif /* RECTANGLE_H_ */
