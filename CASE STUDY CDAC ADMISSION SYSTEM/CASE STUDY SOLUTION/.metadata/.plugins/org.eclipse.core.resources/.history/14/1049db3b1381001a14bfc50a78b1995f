#include<iostream>
#include<fstream>
#include "Student.h"
#include "Center.h"
#include "Course.h"

#include "AdmissionSystem.h"
#include "capacities.h"

using namespace std;

void load_Files(AdmissionSystem & a)
{
	ifstream if_capacities("file/capacities.csv");

	if(!if_capacities)
	{
		cout<<"Failed to open file"<<endl;
		return ;
	}

}


int main()
{
	AdmissionSystem a;

	load_Files(a);

	a.capacitie.push_back(capacities());
	a.display_capacities();

	return 0;
}


