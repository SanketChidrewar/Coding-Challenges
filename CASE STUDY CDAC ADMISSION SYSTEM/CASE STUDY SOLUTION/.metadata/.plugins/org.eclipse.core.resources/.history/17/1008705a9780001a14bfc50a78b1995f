#include "Course.h"
#include<cstring>
#include<iostream>
using namespace std;

Course::Course() {
	// TODO Auto-generated constructor stub
	this->id=0;
	this->fees=0;
	strcpy(this->name," ");
	this->section = ' ';
}
Course::Course(int id, char* name, double fees, char section)
{
	this->id=id;
	this->fees=fees;
	strcpy(this->name,name);
	this->section = section;
}

double Course::getFees() const {
	return fees;
}
void Course::setFees(double fees) {
	this->fees = fees;
}

int Course::getId() const {
	return id;
}
void Course::setId(int id) {
	this->id = id;
}

const char* Course::getName() const {
	return name;
}
void Course::setName(char* name)
{
	strcpy(this->name,name);
}

