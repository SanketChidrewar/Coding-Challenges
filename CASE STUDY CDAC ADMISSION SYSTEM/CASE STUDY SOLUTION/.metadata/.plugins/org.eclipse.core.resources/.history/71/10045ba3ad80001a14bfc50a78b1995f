#ifndef CAPACITIES_H_
#define CAPACITIES_H_

#include<iostream>
using namespace std;

class capacities {
private:
	string center_id;
	string course_name;
	int capacity;
	int filled_capacity;

public:
	capacities();
	capacities(string center_id, string course_name, int capacity, int filled_capacity);
	virtual ~capacities();
};

#endif /* CAPACITIES_H_ */
