#ifndef COURSES_H_
#define COURSES_H_

#include<iostream>
using namespace std;

class courses {
private:
	int id;
	string name;
	double fees;
	string section;
public:
	courses();
	courses(int id, string name, double fees, string section);


	void display_courses();

	virtual ~courses();
};

#endif /* COURSES_H_ */
